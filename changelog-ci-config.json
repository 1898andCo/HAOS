{
    "header_prefix": "Version:",
    "commit_changelog": true,
    "comment_changelog": true,
    "pull_request_title_regex": "^Release",
    "version_regex": "[a-zA-Z]+?([0-9]{1,2})+[.]+([0-9]{1,2})+[.]+([0-9]{1,2})\\s\\(\\d{1,2}-\\d{1,2}-\\d{4}\\)",
    "group_config": [
        {
            "title": "Bug Fixes",
            "labels": [
                "bug",
                "bugfix",
                "fix"
            ]
        },
        {
            "title": "Code Improvements",
            "labels": [
                "improvements",
                "enhancement",
                "refactor",
                "chore"
            ]
        },
        {
            "title": "New Features",
            "labels": [
                "feature"
            ]
        },
        {
            "title": "Documentation Updates",
            "labels": [
                "docs",
                "documentation",
                "doc"
            ]
        }
    ]
}